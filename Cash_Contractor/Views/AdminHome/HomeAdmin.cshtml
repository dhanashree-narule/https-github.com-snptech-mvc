<body>
    <div class="header">
        <div class="row">
            <div class="col-12 col-md-4 logo">
                <img src="~/assets/images/logo.jpg" alt="Ahmed Pro Partner" />
            </div>
            <div class="col-12 col-md-6 text-right align-self-center ml-auto">
                <a href="@Url.Action("AddUser", "AddUser")" class="btn btn-dark menu-btn"><i class="fa fa-user-o" aria-hidden="true"></i> Create User Login</span></a>
                <a href="@Url.Action("AddClaim", "AddClaim")" class="btn btn-dark menu-btn"><i class="fa fa-folder-open-o" aria-hidden="true"></i> Open/View Claims</span></a>
                <a href="@Url.Action("AddClaim", "AddClaim")" class="btn btn-dark menu-btn"><i class="fa fa-file-text-o" aria-hidden="true"></i> New Claim Entry</span></a>
                <a href="@Url.Action("LogIn", "Loging")" class="btn btn-danger exit-btn">Exit <i class="fa fa-sign-out" aria-hidden="true"></i></a>
            </div>
        </div>
    </div>
    <div class="home_screen">
        <div class="login100-form-title p-b-51">
            <img src="~/assets/images/partner_logo.png" alt="Ahmed Pro Partner" />
        </div>
        <div class="product_image">
            <h2>Ducted Splits Unit</h2>
            <img src="~/assets/images/raheem_products.jpg" alt="SAGN + EL3T" />
